						"exec": [
							"// Получаем ответ в виде текста\r",
							"const responseBody = pm.response.text();\r",
							"\r",
							"// Используем регулярное выражение для извлечения всех содержимых элементов с классом ymcKnRT6\r",
							"const regex = /<div class=\"ymcKnRT6\">(.*?)<\\/div>/g;\r",
							"let matches;\r",
							"let values = [];\r",
							"\r",
							"// Ищем все совпадения\r",
							"while ((matches = regex.exec(responseBody)) !== null) {\r",
							"    // Добавляем найденное значение в массив\r",
							"    values.push(matches[1].trim());\r",
							"}\r",
							"\r",
							"// Пронумеровываем значения\r",
							"const numberedValues = values.map((value, index) => `${index + 1}. ${value}`);\r",
							"\r",
							"// Объединяем все пронумерованные значения в одну строку, разделенную пробелами\r",
							"const combinedValues = numberedValues.join(' ');\r",
							"\r",
							"// Сохраняем объединенные пронумерованные значения в переменной окружения\r",
							"pm.environment.set(\"ymcKnRT6_values5\", combinedValues);\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
